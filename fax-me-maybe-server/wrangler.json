{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "fax-me-maybe-server",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"routes": [
		{
			"pattern": "remind.deadpackets.pw",
			"custom_domain": true
		}
	],
	"upload_source_maps": true,
	"assets": {
		"directory": "./dist/client",
		"not_found_handling": "single-page-application"
	},
	"d1_databases": [
		{
			"binding": "faxmemaybe_db",
			"database_name": "faxmemaybe-db",
			"database_id": "97ce92cc-6c9f-495e-9c4d-121c1ce12e3e"
		}
	],
    "browser": {
      "binding": "CLOUDFLARE_BROWSER"
    },
    "r2_buckets": [
      {
        "binding": "TICKET_BUCKET",
        "bucket_name": "fax-me-maybe-tickets",
        "preview_bucket_name": "fax-me-maybe-tickets"
      }
    ],
    "ratelimits": [
        {
            "name": "RATE_LIMITER",
            "namespace_id": "1001",
            "simple": {
                "limit": 10,
                "period": 60
            }
        }
    ]
}